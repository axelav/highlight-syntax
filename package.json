{
  "name": "highlight-syntax-es6",
  "version": "3.0.2",
  "description": "fork of substack's highlight-syntax package that includes es6 features - highlight source code syntax with html",
  "main": "index.js",
  "bin": {
    "highlight-syntax": "bin/cmd.js"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "dependencies": {
    "concat-stream": "^1.5.2",
    "minimist": "^1.2.0",
    "outpipe": "^1.1.1",
    "tokenizer-array": "^1.1.0"
  },
  "devDependencies": {
    "marked": "^0.3.6",
    "tape": "^4.6.2"
  },
  "keywords": [
    "highlight",
    "syntax",
    "javascript",
    "js",
    "bash",
    "shell",
    "sh"
  ],
  "scripts": {
    "css": "cat */dark.css > dark.css && cat */light.css > light.css",
    "test": "tape test/*.js"
  },
